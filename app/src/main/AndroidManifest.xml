        <activity android:name=".BankViewSample.BankViewActivity" />
        <activity android:name=".BitmapShaderSample.BitmapShaderActivity" />
        <activity android:name=".GiftViewSample.GiftActivity" />
        <activity android:name=".HandlerThreadSample.HandlerThreadActivity" />
        <activity android:name=".IntentServiceSample.IntentServiceActivity" />

        <service
            android:name=".IntentServiceSample.UploadImgService"
            android:exported="false"></service>
    </application>

</manifest>