<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.zhoushicheng.myapplication.StarRatingSample.RatingStarActivity">

    <com.example.zhoushicheng.myapplication.StarRatingSample.RatingStarView
        android:id="@+id/star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        app:starBackground="@mipmap/unstar"
        app:starClickable="false"
        app:starDistance="3dp"
        app:starDrawBackground="@mipmap/star"
        app:starHeight="18dp"
        app:starWidth="18dp"
        app:starsNum="5" />

</RelativeLayout>
